proc std::math::atan2(float, float) -> float = gera_std_math_atan2

/// Returns the hyperbolic sine of the given float.
proc std::math::sinh(float) -> float = gera_std_math_sinh
/// Returns the hyperbolic cosine of the given float.
proc std::math::cosh(float) -> float = gera_std_math_cosh
/// Returns the hyperbolic tangent of the given float.
proc std::math::tanh(float) -> float = gera_std_math_tanh
