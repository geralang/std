proc std::math::asinh(float) -> float = gera_std_math_asinh
/// Returns the inverse hyperbolic cosine of the given float.
proc std::math::acosh(float) -> float = gera_std_math_acosh
/// Returns the inverse hyperbolic tangent of the given float.
proc std::math::atanh(float) -> float = gera_std_math_atanh

/// Returns the given float rounded to the nearest integer
/// towards positive infinity (rounding up).
proc std::math::ceil(float) -> float = gera_std_math_ceil
/// Returns the given float rounded to the nearest integer
/// towards negative infinity (rounding down).
proc std::math::floor(float) -> float = gera_std_math_floor
/// Returns the given float rounded to the nearest integer
/// towards zero (rounding up if negative and down if positive).
proc std::math::trunc(float) -> float = gera_std_math_trunc
/// Returns the given float rounded to the nearest integer
/// (rounding to whatever integer is closest).
proc std::math::round(float) -> float = gera_std_math_round
